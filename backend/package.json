{
  "name": "rapid-backend",
  "version": "1.0.0",
  "description": "RAPID (Review & Assessment Powered by Intelligent Documentation) Backend",
  "main": "dist/api/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "prisma": {
    "seed": "tsx src/scripts/seed-db.ts"
  },
  "scripts": {
    "dev": "tsx watch src/api/index.ts",
    "build": "tsc",
    "start": "node dist/api/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,js,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md,yml,yaml}\"",
    "remove:unused": "eslint . --ext .ts --fix --fix-type problem"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.787.0",
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/client-secrets-manager": "^3.777.0",
    "@aws-sdk/client-sfn": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@fastify/cors": "^11.0.1",
    "@prisma/client": "^6.6.0",
    "fastify": "^5.3.2",
    "jose": "^5.10.0",
    "mysql2": "^3.6.1",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.1.91",
    "prisma": "^6.6.0",
    "ulid": "^2.4.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.148",
    "@types/node": "^20.8.6",
    "@types/pdfjs-dist": "^2.10.378",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-unused-exports": "^11.0.1",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vitest": "^3.2.2"
  }
}
